{
    "name": "vue3-template",
    "private": true,
    "version": "0.0.0",
    "type": "module",
    "gitHooks": {
        "pre-commit": "lint-staged"
    },
    "lint-staged": {
        "*.{js,vue,jsx.ts?(x)}": [
            "prettier --write --ignore-unknown"
        ]
    },
    "scripts": {
        "dev": "vite",
        "build": "vue-tsc && vite build",
        "preview": "vite preview",
        "prepare": "husky install",
        "scriptname": "cmd"
    },
    "dependencies": {
        "nprogress": "^0.2.0",
        "pinia": "^2.0.23",
        "sass": "^1.56.0",
        "screenfull": "^6.0.2",
        "uuid": "^9.0.0",
        "vue": "^3.2.41",
        "vue-router": "4"
    },
    "devDependencies": {
        "@vicons/antd": "^0.12.0",
        "@vicons/fluent": "^0.12.0",
        "@vicons/utils": "^0.1.4",
        "@vitejs/plugin-vue": "^3.2.0",
        "autoprefixer": "^10.4.13",
        "husky": "^8.0.2",
        "less": "^4.1.3",
        "lint-staged": "^13.0.3",
        "naive-ui": "^2.33.5",
        "postcss": "^8.4.18",
        "prettier": "2.7.1",
        "tailwindcss": "^3.2.1",
        "typescript": "^4.6.4",
        "vite": "^3.2.0",
        "vue-tsc": "^1.0.9"
    }
}
